{
  "name": "dozenmatter",
  "version": "2.1.1",
  "description": "Mattes a dozen or less images",
  "repository": {
    "url": "https://github.com/dbushong/dozenmatter",
    "type": "git"
  },
  "build": {
    "appId": "com.floatingsheep.dozenmatter",
    "linux": {
      "target": ["AppImage"]
    },
    "productName": "DozenMatter",
    "publish": [
      "github"
    ]
  },
  "author": "David Bushong <david@bushong.net>",
  "license": "MIT",
  "main": "main.js",
  "engines": {
    "node": ">=10.12.0"
  },
  "scripts": {
    "watch-stylus": "stylus -w index.styl",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "release": "build",
    "test": "eslint ."
  },
  "devDependencies": {
    "electron": "^11.0.2",
    "electron-builder": "^22.9.1",
    "eslint": "^7.0.0",
    "eslint-config-groupon": "^10.0.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "electron-prompt": "^1.6.0"
  }
}